using System;

namespace ConsoleApplication2
{
    class Program
    {
        static void Main(string[] args)
        {
            var s ;
            s = Console.ReadLine();
            if (s.Length > 12) s = s.Substring(0, 6);
            else
            {
                for (var i = s.Length; i < 10; i++)
                    s += 'o';
            }
            Console.WriteLine(s);
            Console.ReadLine();
        }
    }
}
